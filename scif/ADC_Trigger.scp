        <sccode name="initialize" init_power_mode="0"><![CDATA[//////////// ADC CONFIG ////////////

// Select ADC input
adcSelectGpioInput(AUXIO_A_ADC_INPUT);

// Enable ADC with manual trigger
adcEnableSync(ADC_REF_FIXED, ADC_SAMPLE_TIME_2P7_US, ADC_TRIGGER_MANUAL);

state.adcEnabled = 1;

/////////// TIMER0 CONFIG ////////////

// Start Timer 0 in periodic mode: 4 kHz clock, 4 ticks = 1 ms
timer0StartWithTickSrc(TIMER0_TICKSRC_AON_RTC_4KHZ, 1, TIMER0_MODE_PERIODICAL, cfg.frequency, 0);

state.timer0Running = 1;

evhSetupTimer0CustomTrigger(0);]]></sccode>
        <sccode name="terminate" init_power_mode="0"><![CDATA[// TERMINATION CODE

if(state.timer0Running == 1) {
     timer0Stop();
     state.timer0Running = 0;
}

evhCancelTrigger(0);

if (state.adcEnabled == 1) {
    adcFlushFifo();
    adcDisable();
    state.adcEnabled = 0;
}]]></sccode>
