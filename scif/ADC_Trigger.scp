        <sccode name="event0" init_power_mode="0"><![CDATA[////////////////EVENT HANDLER A CODE//////////////////

//gpioSetOutput(AUXIO_O_DEBUG);
U16 counter = 0;
U16 matrixSize = cfg.matrixSize;

while (state.sensor < matrixSize) {

    adcGenManualTrigger();
    U16 n = state.head;
    adcReadFifo(output.pSamples[n]);

    utilIncrAndWrap(n, BUFFER_SIZE; state.head);     // Update the buffer head index

    if (state.sensor & 1) {
        gpioSetOutput(AUXIO_O_ROW0);
    } else {
        gpioClearOutput(AUXIO_O_ROW0);
    }

    if (state.sensor & 2) {
        gpioSetOutput(AUXIO_O_ROW1);
    } else {
        gpioClearOutput(AUXIO_O_ROW1);
    }

    if (state.sensor & 4) {
        gpioSetOutput(AUXIO_O_ROW2);
    } else {
        gpioClearOutput(AUXIO_O_ROW2);
    }

    if (state.sensor & 8) {
        gpioSetOutput(AUXIO_O_COL0);
    } else {
        gpioClearOutput(AUXIO_O_COL0);
    }

    if (state.sensor & 16) {
        gpioSetOutput(AUXIO_O_COL1);
    } else {
        gpioClearOutput(AUXIO_O_COL1);
    }

    if (state.sensor & 32) {
        gpioSetOutput(AUXIO_O_COL2);
    } else {
        gpioClearOutput(AUXIO_O_COL2);
    }

    fwDelayUs(1);    // mux settling time delay
    //utilIncrAndSat(state.sensor, MATRIX_SIZE ; state.sensor);   // Update the matrix index
    state.sensor = state.sensor +1;
}

//gpioClearOutput(AUXIO_O_DEBUG);

state.sensor = 0;

if (state.alertEnabled == 1) {

    // Calculate the number of samples currently stored
    S16 count = state.head - state.tail;

    if (count < 0) {
        count += BUFFER_SIZE;
    }

    // If above the configured threshold ...
    if (count >= cfg.alertThr) {
        gpioToggleOutput(AUXIO_O_DEBUG);
        fwGenQuickAlertInterrupt();
        state.alertEnabled = 0;
    }
}

evhSetupTimer0CustomTrigger(0); // Setup the next event trigger (event index 0)]]></sccode>
        <sccode name="event1" init_power_mode="0"><![CDATA[]]></sccode>
        <sccode name="event2" init_power_mode="0"><![CDATA[]]></sccode>
